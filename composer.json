{
    "name": "hamed/countries",
    "description": "A PHP client for the RestCountries API",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Hamed Haghi",
            "email": "hamedhaghi07@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hamed\\Countries\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hamed\\Countries\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "require": {
        "guzzlehttp/guzzle": "^6.5",
        "symfony/property-access": "^3.4",
        "symfony/serializer": "^3.4",
        "vlucas/phpdotenv": "^4.3"
    },
    "config": {
        "sort-packages": true
    }
}
